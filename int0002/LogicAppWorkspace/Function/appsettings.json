{
  "Serilog": {
    "Using": [ "Nodinite.Serilog.AzureBlobStorageSink" ],
    "WriteTo": [
      {
        "Name": "NodiniteAzureBlobStorageSinkWithConnectionString",
        "Args": {
          "ConnectionString":"%NodiniteStorageAccountConnectionString%",
          "ContainerName":"%NodiniteFunctionLoggingContainerName%",
          "Settings": {
            "LogAgentValueId": "4",
            "EndPointDirection": 0,
            "EndPointTypeId": 75,
            "OriginalMessageTypeName": "REPLACEDBYCODE",
            "LogOnlyMessagesWithBody": "false"
          }
        }
      }
    ]
  }
}